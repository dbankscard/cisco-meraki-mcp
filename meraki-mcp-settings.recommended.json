{
  "autoApprove": {
    "enabled": true,
    "tools": {
      "all": false,
      "patterns": [
        "*_list",
        "*_get",
        "*_statuses",
        "*_overview",
        "network_traffic_get",
        "network_events_list",
        "organization_devices_*",
        "organization_licenses_overview"
      ],
      "specific": [],
      "exclude": [
        "network_delete",
        "network_split",
        "network_combine",
        "organization_admin_create",
        "organization_license_claim",
        "organization_inventory_claim"
      ]
    },
    "readOnlyByDefault": true
  },
  "rateLimit": {
    "requestsPerSecond": 5,
    "burstSize": 10
  },
  "logging": {
    "level": "info",
    "logApiCalls": true
  }
}